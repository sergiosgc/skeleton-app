{
    "name": "example.com/app",
    "description": "The description",
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/fs-rest-router"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/html-components"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/jsonschema-form"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/rest"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/php-array"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/config-trait"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/crud-trait"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/auth.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/php-sprintf"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergiosgc/translation"
        },
        {
            "type": "vcs",
