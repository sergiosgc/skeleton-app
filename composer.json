        {
            "type": "vcs",
            "url": "git@github.com:sergiosgc/api-documents.git"
        }
    ],
    "require": {
        "sergiosgc/rest-router": "dev-master",
        "sergiosgc/html-components": "dev-master",
        "sergiosgc/jsonschema-form": "dev-master",
        "sergiosgc/php-array": "dev-master",
        "sergiosgc/sprintf": "dev-master",
        "sergiosgc/crud-trait": "dev-master",
        "sergiosgc/config-trait": "dev-master",
        "sergiosgc/auth": "dev-master",
        "sergiosgc/rest": "dev-master",
        "sergiosgc/translation": "dev-main",
        "sergiosgc/localization": "dev-master",
        "sergiosgc/api-documents": "dev-main",
        "phpseclib/phpseclib": "3.0.*"
    },
    "autoload": {
        "files": [ "lib/functions.php", "lib/app/Exception.php", "lib/app/functions.php" ],
